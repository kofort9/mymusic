{
  "name": "mymusic-dj-assistant",
  "version": "2.0.54",
  "description": "Real-Time DJ Assistant MVP",
  "main": "dist/main.js",
  "bin": {
    "spotifydj": "./bin/spotifydj"
  },
  "scripts": {
    "start": "ts-node src/main.ts",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\""
  },
  "keywords": ["spotify", "dj", "mixing", "camelot-wheel", "harmonic-mixing", "music", "cli"],
  "author": "",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/kofifort/mymusic-dj-assistant"
  },
  "bugs": {
    "url": "https://github.com/kofifort/mymusic-dj-assistant/issues"
  },
  "homepage": "https://github.com/kofifort/mymusic-dj-assistant#readme",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.19",
    "@types/spotify-web-api-node": "^5.0.11",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "axios": "^1.6.7",
    "chalk": "^4.1.2",
    "dotenv": "^16.4.4",
    "express": "^4.18.2",
    "open": "^8.4.2",
    "spotify-web-api-node": "^5.0.2",
    "zod": "^3.22.4"
  }
}
